public class Ejercicio3 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int contador=0;
	
		while (contador<=100){
			if (contador%2==0){
				System.out.println(contador);
				contador++;
			
			
		}
		
	}
	}
}

