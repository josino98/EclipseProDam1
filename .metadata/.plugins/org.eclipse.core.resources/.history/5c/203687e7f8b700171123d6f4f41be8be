public class Ejercicio16 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int serie=0;
		int contador=1;
		int numero=0;
		while (serie<=10){
			if (numero<=10){
				contador++;
				numero++;
			}
		System.out.println();
		serie++;
		numero=1;
		}
		
	}

}
