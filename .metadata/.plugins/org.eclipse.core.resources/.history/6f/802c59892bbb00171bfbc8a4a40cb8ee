 */
public class Ejercicio23 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int contadorf=0;
		int contadorc=0;
		int A=0;
		while (A>=100){
			while (contadorc<=11){
				A++;
				System.out.println(A);
				contadorc++;
			}
			contadorc=0;
			contadorf++;
			System.out.println();								
		}
		contadorc=10;
		System.out.println(contadorc);
		System.out.println(contadorf);
	}

}
