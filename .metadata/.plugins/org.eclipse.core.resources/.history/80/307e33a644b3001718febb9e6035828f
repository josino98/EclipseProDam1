/**
 * @author usuario
 *
 */
public class Ejercicio13 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int contador=0;
		int multiplos2=0;
		int multiplos3=0;
		while (contador<=100){
			if (contador%2==0){
				System.out.println(contador);
				multiplos2=contador+multiplos2;
			}
			if (contador%3==0){
				System.out.println(contador);
				multiplos3=contador+multiplos3;
			}
			contador++;
		}
		

	}

}
