 */
public class Ejercicio16 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int serie=0;
		int numero=0;
		while (serie<10){
			numero=1;
			while(numero<=10){
				System.out.println(numero);
				numero++;
			}
			serie++;
		}

	}

}
