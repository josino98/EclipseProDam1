public class Ejercicio4 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int suma=0; int contador=1;
		while (contador<100) {
			contador++;
			suma=suma+contador; 
			contador++;
			System.out.println(contador);
			}
	
	}
}


		